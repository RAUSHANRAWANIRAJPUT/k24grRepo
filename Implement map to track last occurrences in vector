// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int main() {
    vector<int>vec={1,2,3,4,5,6,7,4,2,1};
    int n=vec.size();
    
    map<int,int>mp;
    
    for(int i=0; i<n; i++){
        mp[vec[i]]=i;
    }
    for(auto& x:mp){
        int key=x.first;
        int value=x.second;
        cout<<"key: "<<key<<" occured: "<<value<<endl;
    }
    
   
}
