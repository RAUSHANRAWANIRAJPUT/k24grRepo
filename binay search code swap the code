
#include <iostream>
using namespace std;
    void setdata(int arr[], int &n,int target){
        for(int i=0; i<n; i++){
            for(int j=0; j<n-1; j++){
                if(arr[j]>arr[j+1]){
                    swap(arr[j],arr[j+1]);
                }
            }
        }
    }
    void getdata(int arr[],int n){
        for(int i=0; i<n; i++ ){
            cout<<arr[i]<<" ";
        }
    }

int main() {
    int n;
    cout<<"n: ";
    cin>>n;
    
    cout<<"Array: ";
    int arr[n];
    for(int i=0; i<n; i++){cin>>arr[i];}
    // cout<<endl;
    
    int target;
    cout<<"Target: ";
    cin>>target;
    //call the setdata 
    setdata(arr,n,target);
    //call the getdata;
    getdata(arr,n);
    

    return 0;
}
